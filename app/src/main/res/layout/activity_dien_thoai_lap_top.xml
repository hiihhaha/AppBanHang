<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.DienThoaiLapTopActivity">

    <androidx.appcompat.widget.Toolbar
        app:navigationIcon="@drawable/ic_back_24"
        android:id="@+id/toolbarmanhinhdienthoai"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#F804F8F0"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Điện Thoại" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rcv_dienthoai"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginHorizontal="5dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbarmanhinhdienthoai" />

</androidx.constraintlayout.widget.ConstraintLayout>